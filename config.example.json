{
  "repository": "camunda/camunda-7-to-8-data-migrator",
  "downloadLogs": true,
  "forceDownload": false,
  "logsDirectory": "./files",
  "cacheGHRequests": true,
  "workflows": [
    {
      "name": "CI Pipeline",
      "enabled": true,
      "fetchLastRuns": 5,
      "workflowFileName": "ci.yml"
    }
  ],
  "singleRun": {
    "enabled": false,
    "url": ""
  },
  "llm": {
    "enabled": true,
    "provider": "openai",
    "model": "gpt-4o",
    "maxTokens": 1000,
    "temperature": 0.1,
    "confidenceThreshold": 0.8,
    "fallbackToPattern": true
  }
}
